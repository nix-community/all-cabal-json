Executable: hg-buildpackage
Extensions: ExistentialQuantification, OverlappingInstances,
    UndecidableInstances
GHC-Options: -O2
Main-Is: hg-buildpackage.hs

Executable: hg-markdeb
Extensions: ExistentialQuantification, OverlappingInstances,
    UndecidableInstances
GHC-Options: -O2
Main-Is: hg-markdeb.hs

