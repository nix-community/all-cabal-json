library
    exposed-modules:
        Zifter.Hlint
    build-depends:
        base >=4.9 && <=5,
        filepath >=1.4 && <1.5,
        hlint >=1.9.35 && <1.10,
        path >=0.5 && <0.6,
        path-io >=1.2 && <1.3,
        safe >=0.3 && <0.4,
        zifter >=0.0 && <0.1
    default-language: Haskell2010
    hs-source-dirs: src/
    ghc-options: -Wall

