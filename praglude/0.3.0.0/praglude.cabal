name: praglude
version: 0.3.0.0
synopsis: A pragmatic Prelude
description: An opinionated alternative Prelude, focused on practicality.
homepage: https://github.com/tdammers/praglude
license: BSD3
license-file: LICENSE
author: Tobias Dammers
maintainer: tdammers@gmail.com
copyright: 2016 Tobias Dammers
category: Web
build-type: Simple
cabal-version: >=1.10

library
  hs-source-dirs: src
  exposed-modules: Praglude
  build-depends: base >=4.9 && <5
               , aeson >=0.11.2.1 && <0.12
               , bytestring >=0.10.8.1 && <0.11
               , base64-bytestring >=1.0.0.1 && <1.1
               , casing >=0.1.0.1 && <0.2
               , containers >=0.5.7.1 && <0.6
               , data-default >=0.7.1.1 && <0.8
               , deepseq >=1.4.2.0 && <1.5
               , directory >=1.2.6.2 && <1.3
               , filepath >=1.4.1.0 && <1.5
               , hashable >=1.2.4.0 && <1.3
               , lens ==4.14.*
               , template-haskell >=2.11.0.0 && <2.12
               , mtl >=2.2.1 && <2.3
               , random ==1.1.*
               , semigroups >=0.18.2 && <0.19
               , string-convert >=3.0.1 && <3.1
               , text >=1.2.2.1 && <1.3
               , time >=1.1 && <2
               , unordered-containers >=0.2.7.1 && <0.3
               , vector >=0.11.0.0 && <0.12
  default-language: Haskell2010

source-repository head
  type: git
  location: https://github.com/tdammers/praglude
