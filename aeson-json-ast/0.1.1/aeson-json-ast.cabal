name:
  aeson-json-ast
version:
  0.1.1
synopsis:
  Integration layer for "json-ast" and "aeson"
description:
homepage:
  https://github.com/sannsyn/aeson-json-ast 
bug-reports:
  https://github.com/sannsyn/aeson-json-ast/issues 
author:
  Nikita Volkov <nikita.y.volkov@mail.ru>
maintainer:
  Nikita Volkov <nikita.y.volkov@mail.ru>
copyright:
  (c) 2016, Sannsyn AS
license:
  MIT
license-file:
  LICENSE
build-type:
  Simple
cabal-version:
  >=1.10


source-repository head
  type:
    git
  location:
    git://github.com/sannsyn/aeson-json-ast.git


library
  hs-source-dirs:
    library
  default-language:
    Haskell2010
  other-modules:
  exposed-modules:
    Data.Aeson.JSONAST
  build-depends:
    json-ast >= 0.2 && < 0.4,
    aeson >= 0.8 && < 2,
    base >= 4.6 && < 5
