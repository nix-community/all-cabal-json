-- Initial hsbc.cabal generated by cabal init.  For further documentation, 
-- see http://haskell.org/cabal/users-guide/
name:                hsbc
version:             0.1.1.0
synopsis:            A command line calculator
description:         A command line calculator
license:             MIT
license-file:        LICENSE
author:              George Rogers
maintainer:          grogers385@gmail.com
-- copyright:
category:            Utilities
build-type:          Simple
extra-source-files:  ChangeLog.md
cabal-version:       >=1.10

source-repository head
    type: darcs
    location: https://hub.darcs.net/georgerogers42/hsbc

executable hsbc
  main-is:             Main.hs
  -- other-modules:
  -- other-extensions:
  build-depends:       base >=4.9 && <4.10
                     , text >=1.0
                     , attoparsec >=0.13
                     , vector >=0.11
  hs-source-dirs:      src
  default-language:    Haskell2010
