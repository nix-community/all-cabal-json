cabal-version:   >= 1.2
build-type:      Simple

library
    exposed-modules: 
      Network.Delicious.Types,
      Network.Delicious.User,
      Network.Delicious.RSS,
      Network.Delicious.JSON,
      Network.Delicious.Fetch,
      Network.Delicious


    build-depends:   base,
                     xml        >= 1.2.6,
                     feed       >= 0.3.1,
                     curl       >= 1.1,
                     bytestring,
                     nano-md5,
                     json       >= 0.3.3

    ghc-options:     -Wall

