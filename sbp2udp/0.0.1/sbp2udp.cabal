name:                  sbp2udp
version:               0.0.1
license:               BSD3
license-file:        LICENSE
synopsis:              SBP to UDP
description:           SBP to UDP
category:              Network
maintainer:            mark@swift-nav.com
build-type:            Simple
cabal-version:         >=1.10

executable sbp2udp
  main-is:             sbp2udp.hs
  default-language:    Haskell2010
  ghc-options:         -Wall
  build-depends:       base >= 4.7 && < 5
                     , basic-prelude
                     , binary
                     , binary-conduit
                     , bytestring
                     , conduit
                     , conduit-extra
                     , network
                     , optparse-generic
                     , protolude
                     , resourcet
                     , sbp
                     , streaming-commons
