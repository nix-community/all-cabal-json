category:            Science
build-type:          Simple
cabal-version:       >=1.8

library
   exposed-modules: Atmosphere.Dimensional
  -- other-modules:       
   build-depends:   base >= 4 && < 5,
                    atmos >= 0.1 && < 0.2,
                    dimensional >= 0.10.2 && < 0.11

source-repository head
  type:     git
  location: git://github.com/ghorn/atmosphere.git
