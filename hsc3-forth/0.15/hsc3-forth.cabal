                   containers,
                   directory,
                   filepath,
                   hashable,
                   hosc == 0.15.*,
                   hsc3 == 0.15.*,
                   hsc3-db == 0.15.*,
                   hsc3-dot == 0.15.*,
                   mtl,
                   unix
  hs-source-dirs:  hs
  main-is:         hsc3-forth.hs
  ghc-options:     -Wall -fno-warn-orphans -fwarn-tabs

