name:                naver-translate
version:             0.1.0.1
synopsis:            Interface to Naver Translate
homepage:            https://github.com/dahlia/naver-translate
bug-reports:         https://github.com/dahlia/naver-translate/issues
license:             GPL-3
license-file:        LICENSE
author:              Hong Minhee
maintainer:          hongminhee@member.fsf.org
copyright:           (c) 2015 Hong Minhee
category:            Language
build-type:          Simple
extra-source-files:  README.md
cabal-version:       >=1.10

library
  exposed-modules:     Language.Translate.Naver
  build-depends:       aeson        >=0.7.0.6  && <0.8.0.0,
                       base         >=4.7 && <4.8,
                       iso639,
                       lens         >=4.8 && <4.9,
                       lens-aeson   >=1.0.0.3  && <1.1.0.0,
                       network-uri  >=2.6.0.0  && <3.0.0.0,
                       text         >=1.1.0.0  && <1.2.0.0,
                       wreq         >=0.3.0.1  && <0.4.0.0,
                       random       >=1.0.1.1  && <1.1.0.0
  hs-source-dirs:      src
  default-language:    Haskell2010
