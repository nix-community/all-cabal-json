library
    build-depends:
        base            >= 4            && < 5,
        bytestring      >= 0.10.4.0     && < 1,
        hashable        >= 1.2.2.0      && < 2,
        vector          >= 0.10.9.1     && < 1,
        directory       >= 1.2.1.0      && < 2,
        text            >= 1.1.0.0      && < 2,
        aeson           >= 0.7.0.6      && < 1,
        attoparsec      >= 0.11.3.4     && < 1
    hs-source-dirs:     src
    default-language:   Haskell2010
    exposed-modules:
        Database.Stash
