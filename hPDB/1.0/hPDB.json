  "package-flags": [
    {
      "name": "have-mmap",
      "description": "Use mmap to read input faster.",
      "default": true,
      "manual": false
    },
    {
      "name": "have-sse2",
      "description": "Use -msse2 for faster code.",
      "default": true,
      "manual": false
    },
    {
      "name": "have-text-format",
      "description": "Do not use text-format, since it may require double-conversion\nand thus linking of libstdc++ which may break compilation\ndue to GHC bug #5289:\n\nhttp://ghc.haskell.org/trac/ghc/ticket/5289",
      "default": true,
      "manual": false
    },
    {
      "name": "old-bytestring",
      "description": "Use bytestring before version 0.10 (introduced in GHC 7.6), and define NFData for Data.ByteString yourself.\nDisable for GHC 7.6.",
      "default": false,
      "manual": false
    },
    {
      "name": "old-zlib",
      "description": "Use zlib before version 0.5.4 (introduced in GHC 7.6).\nDisable for GHC 7.6.1",
      "default": false,
      "manual": false
    },
    {
      "name": "old-vector",
      "description": "Use old vector library before version 0.10 (introduced along with GHC 7.6).\nDisable for GHC 7.6.1 and latest 7.4.2.",
      "default": false,
      "manual": false
    }
