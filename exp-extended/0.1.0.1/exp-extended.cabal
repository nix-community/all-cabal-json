name:                exp-extended
version:             0.1.0.1
synopsis:            floating point with extended exponent range
homepage:            http://code.mathr.co.uk/exp-extended
license:             BSD3
license-file:        LICENSE
author:              Claude Heiland-Allen
maintainer:          claude@mathr.co.uk
copyright:           2015 Claude Heiland-Allen
category:            Numeric
build-type:          Simple
cabal-version:       >=1.10

library
  hs-source-dirs:      src
  exposed-modules:     Numeric.ExpExtended
                       Numeric.ExpExtended.Internal
  build-depends:       base == 4.8.*
                     , compensated >= 0.7 && < 0.8
                     , log-domain >= 0.10 && < 0.11
  default-language:    Haskell2010
  other-extensions:    TypeFamilies

source-repository head
  type:     git
  location: http://code.mathr.co.uk/exp-extended.git

source-repository this
  type:     git
  location: http://code.mathr.co.uk/exp-extended.git
  tag:      v0.1.0.1
