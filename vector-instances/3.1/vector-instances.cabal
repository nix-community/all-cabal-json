name:                vector-instances
version:             3.1
x-revision: 1
synopsis:            Orphan Instances for 'Data.Vector'
homepage:            http://github.com/ekmett/vector-instances
bug-reports:         http://github.com/ekmett/vector-instances/issues
license:             BSD3
license-file:        LICENSE
author:              Edward Kmett
maintainer:          ekmett@gmail.com
category:            Data, Data Structures
build-type:          Simple
cabal-version:       >=1.6
extra-source-files: .travis.yml

source-repository head
  type: git
  location: git://github.com/ekmett/vector-instances.git

library
  other-extensions:
    TypeFamilies

  exposed-modules:
    Data.Vector.Instances

  build-depends:
    base          >= 4       && < 5,
    vector        >= 0.10    && < 0.11,
    semigroupoids >= 3.0     && < 3.1,
    semigroups    >= 0.8.3.1 && < 0.9,
    lens          >= 3.2     && < 3.3,
    comonad       >= 3.0     && < 3.1,
    pointed       >= 3.0     && < 3.1,
    keys          >= 3.0     && < 3.1
