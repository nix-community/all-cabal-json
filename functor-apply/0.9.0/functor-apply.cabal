synopsis:      Strong lax semimonoidal endofunctors (Applicative sans pure)
description:   Strong lax semimonoidal endofunctors (Applicative sans pure)
build-type:    Simple

library
  build-depends: 
    base >= 4 && < 4.4,
    transformers >= 0.2.0 && < 0.3,
    containers >= 0.4.0 && < 0.5,
    comonad >= 0.9.0 && < 1.0,
    semigroups >= 0.3.2 && < 0.4

  exposed-modules:
    Data.Functor.Alt,
    Data.Functor.Apply,
    Data.Functor.Bind,
    Data.Semigroup.Foldable,
    Data.Semigroup.Traversable,
    Control.Applicative.Alt
    Control.Comonad.Apply

  ghc-options: -Wall 
