name: opentheory
version: 1.199
category: Formal Methods
synopsis: The standard theory library
license: MIT
license-file: LICENSE
cabal-version: >= 1.8.0.2
build-type: Simple
author: Joe Leslie-Hurd <joe@gilith.com>
maintainer: Joe Leslie-Hurd <joe@gilith.com>
homepage: http://opentheory.gilith.com/?pkg=base
description:
  The standard theory library - this package was automatically generated
  from the OpenTheory package base-1.199

library
  build-depends:
    base >= 4.0 && < 5.0,
    QuickCheck >= 2.4.0.1 && < 3.0,
    opentheory-primitive >= 1.6 && < 2.0
  hs-source-dirs: src
  ghc-options: -Wall
  exposed-modules:
    OpenTheory.List,
    OpenTheory.Natural

test-suite opentheory-test
  type: exitcode-stdio-1.0
  build-depends:
    base >= 4.0 && < 5.0,
    QuickCheck >= 2.4.0.1 && < 3.0,
    opentheory-primitive >= 1.6 && < 2.0
  hs-source-dirs: src
  ghc-options: -Wall
  main-is: Test.hs
