Name:           haskore-vintage
Version:        0.3
License:        BSD3
License-file:   LICENSE
Author:         Paul Hudak
Maintainer:     diatchki@galois.com
Homepage:       http://haskell.org/haskore/
Category:       Music
Synopsis:       The February 2000 version of Haskore.
Description:    The February 2000 version of Haskore.
Build-type:     Simple
Cabal-version: >= 1.6
tested-with: GHC == 7.10.2, GHC == 7.8.4

Extra-source-files:
  Readme
  docs/tutorial.dvi
  docs/tutorial.ps
  examples/Examples.lhs
  examples/ChildSong6.lhs
  examples/Ssf.lhs
  examples/SelfSim.lhs
  examples/NewResolutions.lhs
  examples/Fractals.hs

Library
  hs-source-dirs: src
  Build-depends: base < 5
  Exposed-modules:
    Haskore
    Haskore.Basics
    Haskore.Bitops
    Haskore.GeneralMidi
    Haskore.LoadMidi
    Haskore.Midi
    Haskore.MidiFile
    Haskore.OutputMidi
    Haskore.Performance
    Haskore.ReadMidi
    Haskore.ToMidi
    Haskore.Test

  Other-modules:
    Haskore.IOExtensions
    Haskore.Loader
    Haskore.Monads
    Haskore.Utils

source-repository head
  type:     git
  location: https://github.com/yav/haskore-vintage



