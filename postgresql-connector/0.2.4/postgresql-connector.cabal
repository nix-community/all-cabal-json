name: postgresql-connector
version: 0.2.4
cabal-version: >=1.22
build-type: Simple
license: BSD3
license-file: LICENSE
copyright: 2015 Mark Fine
maintainer: Mark Fine <mark.fine@gmail.com>
homepage: http://github.com/mfine/postgresql-connector
synopsis: Initial project postgresql-connector from stack
description:
    Please see README.md
category: Web
author: Mark Fine

source-repository head
    type: git
    location: https://github.com/mfine/postgresql-connector

library
    exposed-modules:
        Database.PostgreSQL.Connector
    build-depends:
        base >=4.8 && <5,
        bytestring >=0.10.6.0,
        exceptions >=0.8.3,
        lens >=4.13,
        mtl >=2.2.1,
        postgresql-simple >=0.5.3.0,
        resource-pool >=0.2.3.2,
        resourcet >=1.1.9,
        time >=1.5.0.1,
        transformers-base >=0.4.4
    default-language: Haskell2010
    hs-source-dirs: src
    ghc-options: -Wall

