    {
      "name": "pkg-config",
      "description": "Use @pkg-config(1)@ to locate @zlib@ library.",
      "default": true,
      "manual": true
    },
    {
      "name": "have_builtin_prefetch",
      "description": "The cxx compiler has the __builtin_prefetch intrinsic.",
      "default": false,
      "manual": true
    },
    {
      "name": "have_mm_prefetch",
      "description": "Targeting X86 and the compiler has the _mm_prefetch intrinsic.",
      "default": false,
      "manual": true
    },
    {
      "name": "have_sse42",
      "description": "Can be enabled to improve performance of CRC32C if targeting X86 and\nthe compiler has the _mm_crc32_u{8,32,64} intrinsics.",
      "default": false,
      "manual": true
    },
    {
      "name": "have_arm64_crc32c",
      "description": "Targeting ARM and the compiler has the __crc32c{b,h,w,d} and the\nvmull_p64 intrinsics.",
      "default": false,
      "manual": true
    },
    {
      "name": "have_strong_getauxval",
      "description": "The system libraries have the getauxval function in the <sys/auxv.h> header.\nShould be true on Linux and Android API level 20+.",
      "default": false,
      "manual": true
    },
    {
      "name": "have_weak_getauxval",
      "description": "The compiler supports defining getauxval as a weak symbol.\nShould be true for any compiler that supports __attribute__((weak)).",
      "default": false,
      "manual": true
    }
