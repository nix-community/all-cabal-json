name:                mono-foldable
version:             0.1.0.1
synopsis:            Folds for monomorphic containers
description:         Like Data.Foldable, but works with ByteStrings too
homepage:            http://github.com/JohnLato/mono-foldable
license:             BSD3
license-file:        LICENSE
author:              Artyom Kazak, John W. Lato
maintainer:          jwlato@gmail.com
-- copyright:           
category:            Data
build-type:          Simple
cabal-version:       >=1.16

library
  default-language:            Haskell2010
  exposed-modules:     Data.Foldable.Mono
  -- other-modules:       
  build-depends:       base ==4.6.*
                     , bytestring ==0.10.*
                     , text ==0.11.*
                     , vector ==0.10.*
  hs-source-dirs:      src

source-repository head
  type: git
  location: git://github.com/JohnLato/mono-foldable.git
