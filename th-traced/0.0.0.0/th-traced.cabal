-- Initial th-traced.cabal generated by cabal init.  For further 
-- documentation, see http://haskell.org/cabal/users-guide/

name:                th-traced
version:             0.0.0.0
synopsis:            Tracing Q monad computation
description:         Tracing Q monad computation
homepage:            https://github.com/konn/th-traced
license:             BSD3
license-file:        LICENSE
author:              Hiromi ISHII
maintainer:          konn.jinro_at_gmail.com
-- copyright:           
category:            Template Haskell
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

library
  exposed-modules:     Language.Haskell.TH.Traced
  -- other-modules:       
  other-extensions:    GeneralizedNewtypeDeriving, NoMonomorphismRestriction, OverloadedStrings
  build-depends:       base >=4 && <5
                     , mtl >=2.2 && <2.3
                     , containers >=0.5 && <0.6
                     , template-haskell >=2.10
  -- hs-source-dirs:      
  default-language:    Haskell2010

source-repository head
  Type: git
  Location: git://github.com/konn/th-traced.git
