name:
  json-ast
version:
  0.3.1
synopsis:
  Universal JSON AST datastructure
description:
homepage:
  https://github.com/nikita-volkov/json-ast 
bug-reports:
  https://github.com/nikita-volkov/json-ast/issues 
author:
  Nikita Volkov <nikita.y.volkov@mail.ru>
maintainer:
  Nikita Volkov <nikita.y.volkov@mail.ru>
copyright:
  (c) 2016, Nikita Volkov
license:
  MIT
license-file:
  LICENSE
build-type:
  Simple
cabal-version:
  >=1.10


source-repository head
  type:
    git
  location:
    git://github.com/nikita-volkov/json-ast.git


library
  hs-source-dirs:
    library
  default-extensions:
    NoImplicitPrelude, DeriveDataTypeable
  default-language:
    Haskell2010
  other-modules:
  exposed-modules:
    JSONAST
  build-depends:
    -- data:
    vector >= 0.10 && < 0.13,
    text >= 1 && < 2,
    scientific >= 0.3 && < 0.4,
    unordered-containers >= 0.2 && < 0.3,
    -- general:
    base >= 4.6 && < 5
