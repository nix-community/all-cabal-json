    text             >= 1.2      && < 1.3,
    time             >= 1.5.0.1  && < 1.7,
    transformers     >= 0.4.2.0  && < 0.6,
    unix             >= 2.7.1.0  && < 2.8,
    uuid             >= 1.3.12   && < 1.4,
    wai              >= 3.2      && < 3.3,
