                    Bamboo.Theme.Blueprint.Widget.Footer
                    Bamboo.Theme.Blueprint.Widget.Head
                    Bamboo.Theme.Blueprint.Widget.Header
                    Bamboo.Theme.Blueprint.Widget.Helper
                    Bamboo.Theme.Blueprint.Widget.Navigation
