  "package-flags": [
    {
      "name": "useegl",
      "description": "By default, EGL APIs are used to get OpenGL function pointers.\nIf Disabled, use native library instead of EGL. e.g. WGL, GLX or dlsym.",
      "default": true,
      "manual": false
    },
    {
      "name": "staticlinkes3",
      "description": "By default, OpenGL ES 3.0 APIs are dynamic linked.\nIf Enabled, may cause runtime link error on unsupported environment.\nOpenGL ES 2.0 APIs are always statically linked.",
      "default": false,
      "manual": false
    },
    {
      "name": "staticlinkegl15",
      "description": "By default, EGL 1.5 APIs are dynamic linked.\nIf Enabled, may cause runtime link error on unsupported environment.\nEGL 1.4 APIs are always statically linked.",
      "default": false,
      "manual": false
    },
    {
      "name": "buildexample",
      "description": "",
      "default": true,
      "manual": false
    }
