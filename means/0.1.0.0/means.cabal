name:                means
version:             0.1.0.0
synopsis:            calculate varieties of mean/average using semigroup.
description:         calculate varieties of mean/average using semigroup.
license:             MIT
license-file:        LICENSE
author:              winterland1989
maintainer:          winterland1989@gmail.com
copyright:           2016 Winterland
category:            Data
build-type:          Simple
extra-source-files:  ChangeLog.md, README.md
cabal-version:       >=1.10

library
    exposed-modules:     Data.Semigroup.Means
  -- other-modules:       
  -- other-extensions:    
    build-depends:       base == 4.*, semigroups > 0.18
  -- hs-source-dirs:      
    default-language:    Haskell2010
    ghc-options:         -Wall -O2

source-repository head
    type:     git
    location: https://github.com/winterland1989/means.git
