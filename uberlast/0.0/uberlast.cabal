name:                uberlast
version:             0.0
synopsis:            Generate overloaded lenses from plain data declaration
-- description:         
homepage:            https:/github.com/fumieval/uberlast
license:             BSD3
license-file:        LICENSE
author:              Fumiaki Kinoshita
maintainer:          Fumiaki Kinoshita <fumiexcel@gmail.com>
copyright:           (c) Fumiaki Kinoshita 2014
category:            Control
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

library
  exposed-modules:     Control.Lens.Overload
  -- other-modules:       
  other-extensions:    TemplateHaskell, TypeFamilies, MultiParamTypeClasses, DataKinds
  build-depends:       base == 4.*, template-haskell, lens >= 4.4 && <5, tagged
  hs-source-dirs:      src
  default-language:    Haskell2010