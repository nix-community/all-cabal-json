name: avers-api
version: 0.0.17.0
x-revision: 1
cabal-version: >=1.10
build-type: Simple
license: MIT
license-file: LICENSE
copyright: 2016 Tomas Carnecky
maintainer: tomas.carnecky@gmail.com
homepage: http://github.com/wereHamster/avers-api
synopsis: Types describing the core and extended Avers APIs
description:
    See README
category: Avers
author: Tomas Carnecky

source-repository head
    type: git
    location: https://github.com/wereHamster/avers-api

library
    exposed-modules:
        Avers.API
    build-depends:
        base >=4.7 && <5,
        aeson >=1.0.2.0 && <1.1,
        avers -any,
        bytestring >=0.10.8.1 && <0.11,
        cookie >=0.4.2.1 && <0.5,
        http-api-data >=0.3.2 && <0.4,
        servant >=0.9.0.1 && <0.10,
        text >=1.2.2.1 && <1.3,
        time >=1.6.0.1 && <1.7,
        vector >=0.11.0.0 && <0.13
    default-language: Haskell2010
    hs-source-dirs: src
    other-modules:
        Avers.API.Types
        Avers.API.Instances
        Avers.API.Credentials
    ghc-options: -Wall

