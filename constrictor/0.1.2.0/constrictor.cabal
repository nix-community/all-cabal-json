name:
  constrictor
version:
  0.1.2.0
synopsis:
  strict versions of many things in base
description:
  This package provides strict versions of many functions in
  base, including many folding functions not present in there.
homepage:
  https://github.com/chessai/constrictor.git
license:
  MIT
license-file:
  LICENSE
author:
  chessai
maintainer:
  chessai1996@gmail.com
copyright:
  2018 (c) chessai 
category:
  Control
build-type:
  Simple
extra-source-files:
  ChangeLog.md
cabal-version:
  >=1.10
tested-with:
    GHC == 7.4.2
  , GHC == 7.8.4
  , GHC == 7.10.3
  , GHC == 8.0.2
  , GHC == 8.2.2
  , GHC == 8.4.2

source-repository head
    type:                git
    branch:              master
    location:            https://github.com/chessai/constrictor.git

library
  hs-source-dirs:
    src 
  exposed-modules:
    Constrictor 
  build-depends:
      base >=4.5 && < 5.0
    , ghc-prim
    , transformers 
  default-language:
    Haskell2010
  ghc-options:
    -Wall -O2
